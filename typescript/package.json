{
    "name": "expressjs-generator-typescript",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "node dist/index.js",
        "start:dev": "node --watch --inspect --env-file=.env dist/index.js",
        "build": "tsc",
        "build:dev": "tsc --watch",
        "lint": "eslint . && prettier . --check",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@types/express": "^5.0.1",
        "@types/node": "^22.14.0",
        "eslint": "^9.24.0",
        "globals": "^16.0.0",
        "prettier": "3.5.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.29.1"
    }
}
